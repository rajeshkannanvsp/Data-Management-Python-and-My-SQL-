<script src="{% static 'src\moment.js' %}"></script>
<script>
	function resSubmit() {
		"{% if iframe and pdf %}"
		document.getElementById("bg-before1").classList.remove("d-block");
		document.getElementById("bg-before1").classList.add("d-none");
		"{% elif iframe and doc %}"
		document.getElementById("bg-before2").classList.remove("d-block");
		document.getElementById("bg-before2").classList.add("d-none");
		"{% elif iframe and docx %}"
		document.getElementById("bg-before3").classList.remove("d-block");
		document.getElementById("bg-before3").classList.add("d-none");
		"{% elif err %}"
		document.getElementById("bg-before4").classList.remove("d-block");
		document.getElementById("bg-before4").classList.add("d-none");
		"{% else %}"
		document.getElementById("bg-before5").classList.remove("d-block");
		document.getElementById("bg-before5").classList.add("d-none");
		"{% endif %}"
		document.getElementById("bg-after").classList.remove("d-none");
		document.getElementById("bg-after").classList.add("d-block");

		document.getElementById("input_form").submit();
	}
	var inputElements = document.querySelectorAll('.change-color-on-value');

	// Add an event listener to each input field
	inputElements.forEach(function (inputElement) {
		inputElement.addEventListener('input', function () {
			// Check if the input field has a value
			if (inputElement.value.trim() !== '') {
				// If it has a value, add the 'filled' class to change the color
				inputElement.classList.add('border-color-change-ok');
				inputElement.classList.remove('border-color-change-no');
			} else {
				// If it doesn't have a value, remove the 'filled' class
				inputElement.classList.add('border-color-change-no');
				inputElement.classList.remove('border-color-change-ok');
			}
		});
	});
	function dropHandler(event, inputId) {
		event.preventDefault();
		var data = event.dataTransfer.getData("text").trim();
		var inputField = document.getElementById(inputId);

		if (inputField.value.length > 0) {
			inputField.value += ', ' + data;
		} else {
			inputField.value = data;
		}
	}
	function allowDrop(event) {
		event.preventDefault();
	}




	function handleDrop(event) {
		event.preventDefault();
		const dateString = event.dataTransfer.getData('text').trim();
		const dateInput = document.getElementById('date-input');
		if (moment(dateString, "YYYY-MM-DD").isValid() && (moment(dateString, "YYYY-MM-DD").format("YYYY-MM-DD").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "YYYY-MM-DD")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "DD-MM-YYYY").isValid() && (moment(dateString, "DD-MM-YYYY").format("DD-MM-YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "DD-MM-YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "MM-DD-YYYY").isValid() && (moment(dateString, "MM-DD-YYYY").format("MM-DD-YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "MM-DD-YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "YYYY-DD-MM").isValid() && (moment(dateString, "YYYY-DD-MM").format("YYYY-DD-MM").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "YYYY-DD-MM")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "DD MMMM YYYY").isValid() && (moment(dateString, "DD MMMM YYYY").format("DD MMMM YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "DD MMMM YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "DD MMM YYYY").isValid() && (moment(dateString, "DD MMM YYYY").format("DD MMM YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "DD MMM YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "MMM D, YYYY").isValid() && (moment(dateString, "MMM D, YYYY").format("MMM D, YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "MMM D, YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "MMM DD, YYYY").isValid() && (moment(dateString, "MMM DD, YYYY").format("MMM DD, YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "MMM DD, YYYY")
			dateInput.value = inputDate.format("DD-MM-YYYY");
		} else if (moment(dateString, "MMMM D, YYYY").isValid() && (moment(dateString, "MMMM D, YYYY").format("MMMM D, YYYY").toLowerCase() == dateString.toLowerCase())) {
			var inputDate = moment(dateString, "MMMM D, YYYY")
