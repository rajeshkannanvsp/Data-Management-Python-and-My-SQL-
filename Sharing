								<div class="col-sm-12 col-md-7">
									<input class="form-control change-color-on-value " type="text" id="inputField15" class="drop-area" ondrop="dropHandler(event, 'inputField15')" ondragover="dragOverHandler(event)" placeholder="Technology" />
								</div>
							</div>
							<div class="form-group row">
								<label class="col-sm-12 col-md-5 col-form-label">Skills</label>
								<div class="col-sm-12 col-md-7">
									<textarea class="form-control change-color-on-value" id="inputField16" class="drop-area" ondrop="dropHandler(event, 'inputField16')" ondragover="dragOverHandler(event)" placeholder="Skills"></textarea>
								</div>
							</div>
							<div class="form-group row mt-4">
								<label class="col-sm-10 col-md-5 col-form-label">Domain</label>
								<div class="col-sm-12 col-md-7">
									<input class="form-control change-color-on-value " type="text" id="inputField17" class="drop-area" ondrop="dropHandler(event, 'inputField17')" ondragover="dragOverHandler(event)" placeholder="Domain" />
								</div>
							</div>
							<div class="form-group row mt-4">
								<label class="col-sm-10 col-md-5 col-form-label">Certifications</label>
								<div class="col-sm-12 col-md-7">
									<input class="form-control change-color-on-value " type="text" id="inputField18" class="drop-area" ondrop="dropHandler(event, 'inputField18')" ondragover="dragOverHandler(event)" placeholder="Certifications" />
								</div>
							</div>
							<div class="form-group row mt-4 ">
								<label class="col-sm-10 col-md-5 col-form-label">Exams</label>
								<div class="col-sm-12 col-md-7">
									<input class="form-control change-color-on-value mb-5" type="text" id="inputField19" class="drop-area" ondrop="dropHandler(event, 'inputField19')" ondragover="dragOverHandler(event)" placeholder="Exams" />
